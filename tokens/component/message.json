{
  "component": {
    "message": {
      "typography": {
        "default": {
          "value": "{app.typography.control.text}",
          "type": "typography"
        }
      },
      "color": {
        "primary": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.component.primary.bg.solid}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.component.primary.bg.translucent}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        },
        "secondary": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.component.secondary.bg.solid}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.secondary.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.component.secondary.bg.translucent}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.secondary.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        },
        "danger": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.component.danger.bg.solid}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.danger.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.component.danger.bg.translucent}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.danger.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        },
        "success": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.component.success.bg.solid}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.success.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.component.success.bg.translucent}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.success.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        },
        "warning": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.component.warning.bg.solid}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.danger.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.component.warning.bg.translucent}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.warning.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        },
        "surface": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.surface.elevation.base.bg.default}",
                "type": "color",
                "$extensions": {}
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.secondary.fg.onTranslucent}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.surface.elevation.base.bg.default}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "0.4",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.secondary.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        }
      },
      "dimension": {
        "box": {
          "height": {
            "default": {
              "value": "{app.dimension.control.height.md} + {component.message.dimension.box.padding.default} * 2",
              "type": "dimension"
            }
          },
          "padding": {
            "default": {
              "value": "{app.dimension.control.padding.md} * 0.25",
              "type": "dimension"
            }
          },
          "gap": {
            "default": {
              "value": "{app.dimension.control.padding.md} * 0.25",
              "type": "dimension"
            }
          },
          "radius": {
            "default": {
              "value": "{app.dimension.control.radius.md} + {component.message.dimension.box.padding.default}",
              "type": "dimension"
            }
          },
          "background": {
            "default": {
              "value": "6rem",
              "type": "dimension"
            }
          }
        },
        "text": {
          "padding": {
            "horizontal": {
              "value": "{app.dimension.control.padding.md}",
              "type": "dimension"
            },
            "vertical": {
              "value": "{app.dimension.control.padding.md} * 0.75",
              "type": "dimension"
            }
          }
        }
      },
      "fontWeight": {
        "default": {
          "value": "{config.fontWeight.medium}",
          "type": "fontWeights"
        }
      }
    }
  }
}