{
  "component": {
    "message": {
      "typography": {
        "text": {
          "value": "{app.typography.control.text}",
          "type": "typography"
        },
        "symbol": {
          "value": "{app.typography.control.symbol}",
          "type": "typography"
        }
      },
      "color": {
        "primary": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.component.primary.bg.solid}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.component.primary.bg.translucent}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        },
        "secondary": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.component.secondary.bg.solid}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.secondary.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.component.secondary.bg.translucent}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.secondary.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        },
        "danger": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.component.danger.bg.solid}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.danger.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.component.danger.bg.translucent}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.danger.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        },
        "disabled": {
          "solid": {
            "bg": {
              "value": "{app.color.component.disabled.bg.solid}",
              "type": "color"
            },
            "fg": {
              "value": "{app.color.component.disabled.fg.solid}",
              "type": "color"
            }
          },
          "translucent": {
            "bg": {
              "value": "{app.color.component.disabled.bg.translucent}",
              "type": "color"
            },
            "fg": {
              "value": "{app.color.component.disabled.fg.solid}",
              "type": "color"
            }
          }
        },
        "success": {
          "solid": {
            "bg": {
              "default": {
                "value": "{app.color.component.success.bg.solid}",
                "type": "color"
              },
              "hover": {
                "value": "{app.color.component.success.bg.solid}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "darken",
                      "value": "{config.color.success.darken.hover}",
                      "space": "hsl"
                    }
                  }
                }
              },
              "active": {
                "value": "{app.color.component.success.bg.solid}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "darken",
                      "value": "{config.color.success.darken.active}",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.success.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "value": "{app.color.component.success.bg.translucent}",
                "type": "color"
              },
              "hover": {
                "value": "{app.color.component.success.bg.translucent}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "{config.color.success.alpha.bg.hover}",
                      "space": "hsl"
                    }
                  }
                }
              },
              "active": {
                "value": "{app.color.component.success.bg.translucent}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "{config.color.success.alpha.bg.active}",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.success.fg.onTranslucent}",
                "type": "color"
              }
            }
          }
        }
      },
      "dimension": {
        "height": {
          "value": "{app.dimension.control.height.md} + {component.message.dimension.padding} * 2",
          "type": "dimension"
        },
        "padding": {
          "value": "{app.dimension.control.padding.md} * 0.25",
          "type": "dimension"
        },
        "gap": {
          "value": "{app.dimension.control.gap.md}",
          "type": "dimension"
        },
        "radius": {
          "value": "{app.dimension.control.radius.md} + {component.message.dimension.padding}",
          "type": "dimension"
        },
        "background": {
          "value": "6rem",
          "type": "dimension"
        }
      },
      "fontSize": {
        "text": {
          "value": "{app.fontSize.control.text.md}",
          "type": "fontSizes"
        }
      }
    }
  }
}