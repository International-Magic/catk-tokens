{
  "component": {
    "slider": {
      "dimension": {
        "track": {
          "height": {
            "sm": {
              "value": "{component.slider.dimension.track.height.md} - {component.slider.config.track.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.config.track.height}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.track.height.md} + {component.slider.config.track.step}",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.slider.dimension.handle.width.sm} * 2 + {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.dimension.handle.width.md} * 2 + {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.handle.width.lg} * 2 + {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.slider.dimension.track.radius.md} - {component.slider.config.track.radius.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.config.track.radius.default}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.track.radius.md} + {component.slider.config.track.radius.step}",
              "type": "dimension"
            }
          },
          "border": {
            "sm": {
              "value": "0rem",
              "type": "dimension"
            },
            "md": {
              "value": "0rem",
              "type": "dimension"
            },
            "lg": {
              "value": "0rem",
              "type": "dimension"
            }
          },
          "blur": {
            "default": {
              "value": "6rem",
              "type": "dimension"
            }
          }
        },
        "handle": {
          "height": {
            "sm": {
              "value": "{component.slider.dimension.track.height.sm} - {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.dimension.track.height.md} - {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.track.height.lg} - {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "({component.slider.dimension.track.height.sm} - {component.slider.config.handle.inset} * 2) * {component.slider.config.handle.ratio}",
              "type": "dimension"
            },
            "md": {
              "value": "({component.slider.dimension.track.height.md} - {component.slider.config.handle.inset} * 2) * {component.slider.config.handle.ratio}",
              "type": "dimension"
            },
            "lg": {
              "value": "({component.slider.dimension.track.height.lg} - {component.slider.config.handle.inset} * 2) * {component.slider.config.handle.ratio}",
              "type": "dimension"
            }
          },
          "offset": {
            "sm": {
              "value": "{component.slider.dimension.handle.width.sm} / 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.dimension.handle.width.md} / 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.handle.width.lg} / 2",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.slider.dimension.track.radius.sm} - {component.slider.config.handle.inset}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.dimension.track.radius.md} - {component.slider.config.handle.inset}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.track.radius.lg} - {component.slider.config.handle.inset}",
              "type": "dimension"
            }
          },
          "inset": {
            "sm": {
              "value": "{component.slider.config.handle.inset}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.config.handle.inset}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.config.handle.inset}",
              "type": "dimension"
            }
          },
          "border": {
            "sm": {
              "value": "0rem",
              "type": "dimension"
            },
            "md": {
              "value": "0rem",
              "type": "dimension"
            },
            "lg": {
              "value": "0rem",
              "type": "dimension"
            }
          }
        },
        "label": {
          "icon": {
            "height": {
              "sm": {
                "value": "{app.fontSize.label.symbol.sm}",
                "type": "dimension"
              },
              "md": {
                "value": "{component.slider.dimension.handle.height.md} * 0.5",
                "type": "dimension"
              },
              "lg": {
                "value": "{component.slider.dimension.handle.height.lg} * 0.5",
                "type": "dimension"
              }
            }
          },
          "text": {
            "padding": {
              "sm": {
                "value": "{component.slider.config.track.height} * 0.5 - {component.slider.config.track.step}",
                "type": "dimension"
              },
              "md": {
                "value": "{component.slider.config.track.height} * 0.5",
                "type": "dimension"
              },
              "lg": {
                "value": "{component.slider.config.track.height} * 0.5 + {component.slider.config.track.step}",
                "type": "dimension"
              }
            }
          },
          "gap": {
            "sm": {
              "value": "{component.slider.config.track.height} * 0.5 - {component.slider.config.track.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.config.track.height} * 0.5",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.config.track.height} * 0.5 + {component.slider.config.track.step}",
              "type": "dimension"
            }
          }
        }
      },
      "boxShadow": {
        "default": {
          "value": "{app.boxShadow.control.default}",
          "type": "boxShadow"
        },
        "pressed": {
          "value": "{app.boxShadow.control.pressed}",
          "type": "boxShadow"
        }
      },
      "fontSize": {
        "icon": {
          "sm": {
            "value": "{app.fontSize.label.symbol.sm}",
            "type": "fontSizes"
          },
          "md": {
            "value": "{component.slider.dimension.label.icon.height.md}",
            "type": "fontSizes"
          },
          "lg": {
            "value": "{component.slider.dimension.label.icon.height.lg}",
            "type": "fontSizes"
          }
        },
        "text": {
          "sm": {
            "value": "{app.fontSize.label.text.sm}",
            "type": "fontSizes"
          },
          "md": {
            "value": "{app.fontSize.label.text.md}",
            "type": "fontSizes"
          },
          "lg": {
            "value": "{app.fontSize.label.text.lg}",
            "type": "fontSizes"
          }
        }
      },
      "config": {
        "track": {
          "height": {
            "value": "{app.dimension.control.height.sm}",
            "type": "other",
            "description": "-0.5 for material design style (handle is offset)"
          },
          "step": {
            "value": "0.75rem",
            "type": "other"
          },
          "radius": {
            "default": {
              "value": "0.75rem",
              "type": "other",
              "description": "for 100% roundness make equal to height md"
            },
            "step": {
              "value": "0.125rem",
              "type": "other"
            }
          }
        },
        "handle": {
          "inset": {
            "value": "0.125rem",
            "type": "other"
          },
          "ratio": {
            "value": "1.25",
            "type": "other",
            "description": "ratio defines width â†’ 1 = square / 1.5 = rectangle"
          },
          "pressed": {
            "value": "0.125",
            "type": "other"
          }
        },
        "label": {
          "padding": {
            "value": "1rem",
            "type": "other"
          }
        }
      },
      "color": {
        "track": {
          "default": {
            "bg": {
              "default": {
                "value": "transparent",
                "type": "color"
              },
              "active": {
                "value": "{app.color.component.neutral.fg.solid}",
                "type": "color",
                "$extensions": {}
              },
              "inactive": {
                "value": "{app.color.component.neutral.bg.translucent}",
                "type": "color",
                "$extensions": {}
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              },
              "inline": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            },
            "border": {
              "default": {
                "value": "transparent",
                "type": "color"
              },
              "active": {
                "value": "transparent",
                "type": "color",
                "$extensions": {}
              }
            }
          },
          "disabled": {
            "bg": {
              "default": {
                "value": "{app.color.component.disabled.bg.checked}",
                "type": "color"
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            },
            "border": {
              "default": {
                "value": "transparent",
                "type": "color"
              }
            }
          }
        },
        "handle": {
          "default": {
            "bg": {
              "default": {
                "value": "{app.color.component.danger.bg.solid}",
                "type": "color"
              }
            },
            "border": {
              "default": {
                "value": "transparent",
                "type": "color"
              }
            }
          },
          "disabled": {
            "bg": {
              "default": {
                "value": "{app.color.component.neutral.fg.onSolid}",
                "type": "color",
                "$extensions": {}
              }
            },
            "border": {
              "default": {
                "value": "transparent",
                "type": "color"
              }
            }
          }
        }
      }
    }
  }
}