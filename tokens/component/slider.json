{
  "component": {
    "slider": {
      "typography": {
        "default": {
          "value": "{app.typography.control.text.default}",
          "type": "typography"
        }
      },
      "dimension": {
        "track": {
          "height": {
            "value": "0.25rem",
            "type": "dimension"
          },
          "padding": {
            "value": "{component.slider.dimension.handle.height} * 0.5",
            "type": "dimension"
          },
          "radius": {
            "value": "0.75rem",
            "type": "dimension"
          }
        },
        "handle": {
          "height": {
            "value": "{component.slider.config.handle.height}",
            "type": "dimension"
          },
          "width": {
            "value": "{component.slider.config.handle.height} * {component.slider.config.handle.ratio}",
            "type": "dimension"
          },
          "radius": {
            "value": "{component.slider.config.handle.radius}",
            "type": "dimension"
          },
          "pressed": {
            "height": {
              "value": "{component.slider.dimension.handle.height} * {component.slider.config.handle.pressed}",
              "type": "dimension"
            },
            "width": {
              "value": "{component.slider.dimension.handle.width} * {component.slider.config.handle.pressed}",
              "type": "dimension"
            }
          }
        }
      },
      "config": {
        "track": {
          "height": {
            "value": "0.25rem",
            "type": "other",
            "description": "-0.5 for material design style (handle is offset)"
          },
          "radius": {
            "value": "0rem",
            "type": "other",
            "description": "for 100% roundness make equal to height md"
          }
        },
        "handle": {
          "height": {
            "value": "1rem",
            "type": "other"
          },
          "radius": {
            "value": "0.5rem",
            "type": "other",
            "description": "for 100% roundness make half of height"
          },
          "ratio": {
            "value": "1",
            "type": "other"
          },
          "pressed": {
            "value": "0.9",
            "type": "other"
          }
        }
      },
      "color": {
        "track": {
          "bg": {
            "active": {
              "value": "{app.color.surface.fg.default}",
              "type": "color",
              "$extensions": {}
            },
            "inactive": {
              "value": "{app.color.surface.fg.subtle}",
              "type": "color",
              "$extensions": {}
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.surface.fg.default}",
              "type": "color"
            }
          },
          "border": {
            "default": {
              "value": "transparent",
              "type": "color"
            }
          }
        },
        "handle": {
          "bg": {
            "default": {
              "value": "{app.color.surface.fg.default}",
              "type": "color"
            }
          },
          "border": {
            "default": {
              "value": "transparent",
              "type": "color"
            }
          }
        }
      },
      "boxShadow": {
        "default": {
          "value": {
            "x": "0",
            "y": "0rem",
            "blur": "0.125rem",
            "spread": "0",
            "color": "#0000001a",
            "type": "dropShadow"
          },
          "type": "boxShadow"
        }
      }
    }
  }
}