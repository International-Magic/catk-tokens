{
  "component": {
    "slider": {
      "fontSize": {
        "symbol": {
          "sm": {
            "value": "{app.fontSize.label.symbol.sm}",
            "type": "fontSizes"
          },
          "md": {
            "value": "{component.slider.dimension.label.symbol.height.md}",
            "type": "fontSizes"
          },
          "lg": {
            "value": "{component.slider.dimension.label.symbol.height.lg}",
            "type": "fontSizes"
          }
        },
        "text": {
          "sm": {
            "value": "{app.fontSize.label.text.sm}",
            "type": "fontSizes"
          },
          "md": {
            "value": "{app.fontSize.label.text.md}",
            "type": "fontSizes"
          },
          "lg": {
            "value": "{app.fontSize.label.text.lg}",
            "type": "fontSizes"
          }
        }
      },
      "typography": {
        "default": {
          "value": "{app.typography.control.text.default}",
          "type": "typography"
        }
      },
      "dimension": {
        "track": {
          "height": {
            "value": "0.25rem",
            "type": "dimension"
          },
          "radius": {
            "value": "0.75rem",
            "type": "dimension"
          }
        },
        "handle": {
          "default": {
            "height": {
              "value": "0.75rem",
              "type": "dimension"
            },
            "width": {
              "value": "0.75rem",
              "type": "dimension"
            },
            "radius": {
              "value": "999px",
              "type": "dimension"
            }
          },
          "pressed": {
            "height": {
              "value": "{component.slider.dimension.handle.default.height} * 0.9",
              "type": "dimension"
            },
            "width": {
              "value": "{component.slider.dimension.handle.default.width} * 0.9",
              "type": "dimension"
            }
          }
        }
      },
      "config": {
        "track": {
          "height": {
            "value": "0.25rem",
            "type": "other",
            "description": "-0.5 for material design style (handle is offset)"
          },
          "radius": {
            "value": "0.75rem",
            "type": "other",
            "description": "for 100% roundness make equal to height md"
          }
        },
        "handle": {
          "height": {
            "value": "0.75rem",
            "type": "other"
          },
          "width": {
            "value": "0.75rem",
            "type": "other"
          },
          "pressed": {
            "value": "{component.slider.config.handle.height} * 0.9",
            "type": "other"
          }
        }
      },
      "color": {
        "track": {
          "bg": {
            "active": {
              "value": "{app.color.surface.fg.default}",
              "type": "color",
              "$extensions": {}
            },
            "inactive": {
              "value": "{app.color.surface.fg.subtle}",
              "type": "color",
              "$extensions": {}
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.surface.fg.default}",
              "type": "color"
            }
          },
          "border": {
            "default": {
              "value": "transparent",
              "type": "color"
            }
          }
        },
        "handle": {
          "bg": {
            "default": {
              "value": "{app.color.surface.fg.default}",
              "type": "color"
            }
          },
          "border": {
            "default": {
              "value": "transparent",
              "type": "color"
            }
          }
        }
      },
      "boxShadow": {
        "default": {
          "value": "{app.boxShadow.component.default}",
          "type": "boxShadow"
        }
      }
    }
  }
}