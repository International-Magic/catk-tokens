{
  "component": {
    "icon": {
      "typography": {
        "default": {
          "value": "{app.typography.control.symbol.default}",
          "type": "typography"
        }
      },
      "dimension": {
        "height": {
          "xs": {
            "value": "{component.icon.dimension.height.sm} - {component.icon.config.height.step}",
            "type": "dimension"
          },
          "sm": {
            "value": "{component.icon.dimension.height.md} - {component.icon.config.height.step}",
            "type": "dimension"
          },
          "md": {
            "value": "{component.icon.config.height.default}",
            "type": "dimension"
          },
          "lg": {
            "value": "{component.icon.dimension.height.md} + {component.icon.config.height.step}",
            "type": "dimension"
          }
        }
      },
      "fontSize": {
        "xs": {
          "value": "{app.fontSize.label.text.xs}",
          "type": "fontSizes"
        },
        "sm": {
          "value": "{app.fontSize.label.text.sm}",
          "type": "fontSizes"
        },
        "md": {
          "value": "{app.fontSize.label.text.md}",
          "type": "fontSizes"
        },
        "lg": {
          "value": "{app.fontSize.label.text.lg}",
          "type": "fontSizes"
        }
      },
      "config": {
        "height": {
          "default": {
            "value": "{app.dimension.control.height.md} / 2",
            "type": "other"
          },
          "step": {
            "value": "0.25rem",
            "type": "other"
          }
        }
      }
    }
  }
}