{
  "component": {
    "toggle": {
      "dimension": {
        "track": {
          "height": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.md} - {component.toggle.dimension.config.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.config.height}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.md} + {component.toggle.dimension.config.step}",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.sm} * 2 - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.height.md} * 2 - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.lg} * 2 - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.toggle.dimension.track.radius.md} - {component.toggle.dimension.config.radiusStep}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.config.radius}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.radius.md} + {component.toggle.dimension.config.radiusStep}",
              "type": "dimension"
            }
          }
        },
        "handle": {
          "height": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.sm} - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.height.md} - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.lg} - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.toggle.dimension.track.width.sm} / 2 - {component.toggle.dimension.config.inset}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.width.md} / 2 - {component.toggle.dimension.config.inset}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.width.lg} / 2 - {component.toggle.dimension.config.inset}",
              "type": "dimension"
            },
            "pressed": {
              "sm": {
                "value": "{component.toggle.dimension.handle.width.sm} + {component.toggle.dimension.handle.width.sm} / 4",
                "type": "dimension"
              },
              "md": {
                "value": "{component.toggle.dimension.handle.width.md} + {component.toggle.dimension.handle.width.md} / 4",
                "type": "dimension"
              },
              "lg": {
                "value": "{component.toggle.dimension.handle.width.lg} + {component.toggle.dimension.handle.width.lg} / 4",
                "type": "dimension"
              }
            }
          },
          "offset": {
            "sm": {
              "value": "{component.toggle.dimension.handle.width.sm} / 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.handle.width.md} / 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.handle.width.lg} / 2",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.toggle.dimension.track.radius.sm} - {component.toggle.dimension.config.inset}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.radius.md} - {component.toggle.dimension.config.inset}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.radius.lg} - {component.toggle.dimension.config.inset}",
              "type": "dimension"
            }
          }
        },
        "config": {
          "height": {
            "value": "{app.dimension.control.height.md} / 2",
            "type": "dimension",
            "description": "-0.5 for material design style"
          },
          "inset": {
            "value": "0.125rem",
            "type": "dimension"
          },
          "step": {
            "value": "0.5rem",
            "type": "dimension"
          },
          "radius": {
            "value": "0.5rem",
            "type": "dimension",
            "description": "for 100% roundness lower last value to 0"
          },
          "radiusStep": {
            "value": "0.125rem",
            "type": "dimension"
          }
        }
      },
      "color": {
        "track": {
          "unchecked": {
            "bg": {
              "default": {
                "value": "{app.color.component.unchecked.bg.translucent}",
                "type": "color"
              },
              "active": {
                "value": "{app.color.component.unchecked.bg.translucent}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "{config.color.neutral.alpha.unchecked.active}",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "checked": {
            "bg": {
              "default": {
                "value": "{app.color.component.primary.bg.solid}",
                "type": "color"
              },
              "active": {
                "value": "{app.color.component.primary.bg.solid}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "darken",
                      "value": "{config.color.primary.darken.active}",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            }
          }
        },
        "handle": {
          "unchecked": {
            "bg": {
              "default": {
                "value": "{app.color.component.unchecked.fg.onSolid}",
                "type": "color"
              },
              "disabled": {
                "value": "{app.color.component.unchecked.fg.onSolid}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "{config.color.neutral.alpha.active}",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "checked": {
            "bg": {
              "default": {
                "value": "{app.color.component.unchecked.fg.onSolid}",
                "type": "color"
              },
              "disabled": {
                "value": "{app.color.component.unchecked.fg.onSolid}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "{config.color.neutral.alpha.active}",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "shadow": {
            "default": {
              "value": "black",
              "type": "color"
            }
          }
        }
      },
      "boxShadow": {
        "default": {
          "value": "{app.boxShadow.control.default}",
          "type": "boxShadow"
        },
        "pressed": {
          "value": [
            {
              "x": "0",
              "y": "0",
              "blur": "0.0625rem",
              "spread": "0",
              "color": "#0000004d",
              "type": "dropShadow"
            }
          ],
          "type": "boxShadow"
        }
      },
      "transition": {
        "handle": {
          "state": {
            "value": "0.5s ease-in-out",
            "type": "other"
          },
          "transform": {
            "value": "1s ease",
            "type": "other"
          }
        },
        "track": {
          "state": {
            "value": "0.5s ease",
            "type": "other"
          }
        }
      }
    }
  }
}