{
  "component": {
    "toggle": {
      "dimension": {
        "track": {
          "height": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.md} - {component.toggle.dimension.config.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{app.dimension.control.height.md} / 3 * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.md} + {component.toggle.dimension.config.step}",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.sm} * 2 - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.height.md} * 2 - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.lg} * 2 - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.toggle.dimension.track.radius.md}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.config.radius}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.radius.md}",
              "type": "dimension"
            }
          }
        },
        "handle": {
          "height": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.sm} - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.height.md} - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.lg} - {component.toggle.dimension.config.inset} * 2",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.toggle.dimension.track.width.sm} / 2 - {component.toggle.dimension.config.inset}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.width.md} / 2 - {component.toggle.dimension.config.inset}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.width.lg} / 2 - {component.toggle.dimension.config.inset}",
              "type": "dimension"
            }
          },
          "offset": {
            "sm": {
              "value": "{component.toggle.dimension.handle.width.sm} / 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.handle.width.md} / 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.handle.width.lg} / 2",
              "type": "dimension"
            }
          }
        },
        "config": {
          "inset": {
            "value": "0.125rem",
            "type": "dimension",
            "description": "-0.5 for material design style"
          },
          "step": {
            "value": "0.5rem",
            "type": "dimension"
          },
          "radius": {
            "value": "0.5rem",
            "type": "dimension"
          }
        }
      }
    }
  }
}