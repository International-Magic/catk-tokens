{
  "component": {
    "toggle": {
      "dimension": {
        "track": {
          "height": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.md} - {component.toggle.dimension.inset.default}",
              "type": "dimension"
            },
            "md": {
              "value": "{app.dimension.label.height.md}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.md} + {component.toggle.dimension.inset.default}",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{app.dimension.label.height.sm} * 2 - {component.toggle.dimension.inset.default}",
              "type": "dimension"
            },
            "md": {
              "value": "{app.dimension.label.height.md} * 2 - {component.toggle.dimension.inset.default}",
              "type": "dimension"
            },
            "lg": {
              "value": "{app.dimension.label.height.lg} * 2 - {component.toggle.dimension.inset.default}",
              "type": "dimension"
            }
          }
        },
        "handle": {
          "height": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.sm} - {component.toggle.dimension.inset.default}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.height.md} - {component.toggle.dimension.inset.default}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.lg} - {component.toggle.dimension.inset.default}",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.sm} - 0.25rem",
              "type": "dimension"
            },
            "md": {
              "value": "({component.toggle.dimension.track.width.md} - {component.toggle.dimension.inset.default}) / 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.lg} - 0.25rem",
              "type": "dimension"
            }
          },
          "offset": {
            "sm": {
              "value": "{component.toggle.dimension.handle.width.sm} / 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.handle.width.md} / 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.handle.width.lg} / 2",
              "type": "dimension"
            }
          }
        },
        "inset": {
          "default": {
            "value": "-0.5rem",
            "type": "dimension",
            "description": "-0.5 for material design style"
          }
        }
      }
    }
  }
}