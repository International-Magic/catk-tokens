{
  "component": {
    "toggle": {
      "dimension": {
        "track": {
          "height": {
            "sm": {
              "value": "{component.slider.dimension.track.height.md} - {component.slider.config.track.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.config.track.height}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.track.height.md} + {component.slider.config.track.step}",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.slider.dimension.handle.width.sm} * 2 + {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.dimension.handle.width.md} * 2 + {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.handle.width.lg} * 2 + {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.slider.dimension.track.radius.md} - {component.slider.config.track.radius.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.config.track.radius.default}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.track.radius.md} + {component.slider.config.track.radius.step}",
              "type": "dimension"
            }
          },
          "border": {
            "sm": {
              "value": "0rem",
              "type": "dimension"
            },
            "md": {
              "value": "0rem",
              "type": "dimension"
            },
            "lg": {
              "value": "0rem",
              "type": "dimension"
            }
          }
        },
        "handle": {
          "height": {
            "sm": {
              "value": "{component.slider.dimension.track.height.sm} - {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.dimension.track.height.md} - {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.track.height.lg} - {component.slider.config.handle.inset} * 2",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "({component.slider.dimension.track.height.sm} - {component.slider.config.handle.inset} * 2) * {component.slider.config.handle.ratio}",
              "type": "dimension"
            },
            "md": {
              "value": "({component.slider.dimension.track.height.md} - {component.slider.config.handle.inset} * 2) * {component.slider.config.handle.ratio}",
              "type": "dimension"
            },
            "lg": {
              "value": "({component.slider.dimension.track.height.lg} - {component.slider.config.handle.inset} * 2) * {component.slider.config.handle.ratio}",
              "type": "dimension"
            },
            "pressed": {
              "sm": {
                "value": "{component.slider.dimension.handle.width.sm} + {component.slider.dimension.handle.width.sm} * {component.slider.config.handle.pressed}",
                "type": "dimension"
              },
              "md": {
                "value": "{component.slider.dimension.handle.width.md} + {component.slider.dimension.handle.width.md} * {component.slider.config.handle.pressed}",
                "type": "dimension"
              },
              "lg": {
                "value": "{component.slider.dimension.handle.width.lg} + {component.slider.dimension.handle.width.lg} * {component.slider.config.handle.pressed}",
                "type": "dimension"
              }
            }
          },
          "offset": {
            "sm": {
              "value": "{component.slider.dimension.handle.width.sm} / 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.dimension.handle.width.md} / 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.handle.width.lg} / 2",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.slider.dimension.track.radius.sm} - {component.slider.config.handle.inset}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.dimension.track.radius.md} - {component.slider.config.handle.inset}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.track.radius.lg} - {component.slider.config.handle.inset}",
              "type": "dimension"
            }
          },
          "inset": {
            "sm": {
              "value": "{component.slider.config.handle.inset}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.config.handle.inset}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.config.handle.inset}",
              "type": "dimension"
            }
          },
          "border": {
            "sm": {
              "value": "0rem",
              "type": "dimension"
            },
            "md": {
              "value": "0rem",
              "type": "dimension"
            },
            "lg": {
              "value": "0rem",
              "type": "dimension"
            }
          }
        },
        "icon": {
          "height": {
            "sm": {
              "value": "{component.slider.dimension.handle.height.sm} * 0.5",
              "type": "dimension"
            },
            "md": {
              "value": "{component.slider.dimension.handle.height.md} * 0.5",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.slider.dimension.handle.height.lg} * 0.5",
              "type": "dimension"
            }
          },
          "padding": {
            "sm": {
              "value": "({component.slider.dimension.handle.width.sm} - {component.slider.dimension.icon.height.sm}) / 2 + 0.0625rem",
              "type": "dimension"
            },
            "md": {
              "value": "({component.slider.dimension.handle.width.md} - {component.slider.dimension.icon.height.md}) / 2 + 0.125rem",
              "type": "dimension"
            },
            "lg": {
              "value": "({component.slider.dimension.handle.width.lg} - {component.slider.dimension.icon.height.lg}) / 2 + 0.0625rem",
              "type": "dimension"
            }
          }
        }
      },
      "color": {
        "unchecked": {
          "track": {
            "default": {
              "bg": {
                "default": {
                  "value": "{app.color.component.neutral.bg.translucent}",
                  "type": "color"
                },
                "active": {
                  "value": "{app.color.component.neutral.bg.translucent}",
                  "type": "color",
                  "$extensions": {
                    "studio.tokens": {
                      "modify": {
                        "type": "alpha",
                        "value": "{config.color.neutral.alpha.bg.active}",
                        "space": "hsl"
                      }
                    }
                  }
                }
              },
              "fg": {
                "default": {
                  "value": "{app.color.component.primary.fg.onSolid}",
                  "type": "color"
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                },
                "active": {
                  "value": "transparent",
                  "type": "color",
                  "$extensions": {}
                }
              }
            },
            "disabled": {
              "bg": {
                "default": {
                  "value": "{app.color.component.disabled.bg.translucent}",
                  "type": "color"
                }
              },
              "fg": {
                "default": {
                  "value": "{app.color.component.disabled.fg.solid}",
                  "type": "color"
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                }
              }
            }
          },
          "handle": {
            "default": {
              "bg": {
                "default": {
                  "value": "{app.color.component.neutral.fg.onSolid}",
                  "type": "color"
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                }
              }
            },
            "disabled": {
              "bg": {
                "default": {
                  "value": "{app.color.component.disabled.fg.onSolid}",
                  "type": "color",
                  "$extensions": {}
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                }
              }
            }
          }
        },
        "checked": {
          "track": {
            "default": {
              "bg": {
                "default": {
                  "value": "{app.color.component.primary.bg.solid}",
                  "type": "color"
                },
                "active": {
                  "value": "{app.color.component.primary.bg.solid}",
                  "type": "color",
                  "$extensions": {
                    "studio.tokens": {
                      "modify": {
                        "type": "darken",
                        "value": "{config.color.primary.darken.active}",
                        "space": "hsl"
                      }
                    }
                  }
                }
              },
              "fg": {
                "default": {
                  "value": "{app.color.component.primary.fg.onSolid}",
                  "type": "color"
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                },
                "active": {
                  "value": "transparent",
                  "type": "color",
                  "$extensions": {}
                }
              }
            },
            "disabled": {
              "bg": {
                "default": {
                  "value": "{app.color.component.disabled.bg.checked}",
                  "type": "color"
                }
              },
              "fg": {
                "default": {
                  "value": "{app.color.component.primary.fg.onSolid}",
                  "type": "color"
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                }
              }
            }
          },
          "handle": {
            "default": {
              "bg": {
                "default": {
                  "value": "{app.color.component.neutral.fg.onSolid}",
                  "type": "color"
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                }
              }
            },
            "disabled": {
              "bg": {
                "default": {
                  "value": "{app.color.component.neutral.fg.onSolid}",
                  "type": "color",
                  "$extensions": {}
                }
              },
              "border": {
                "default": {
                  "value": "transparent",
                  "type": "color"
                }
              }
            }
          }
        }
      },
      "boxShadow": {
        "default": {
          "value": "{app.boxShadow.control.default}",
          "type": "boxShadow"
        },
        "pressed": {
          "value": "{app.boxShadow.control.pressed}",
          "type": "boxShadow"
        }
      },
      "fontSize": {
        "icon": {
          "sm": {
            "value": "{component.slider.dimension.icon.height.sm}",
            "type": "fontSizes"
          },
          "md": {
            "value": "{component.slider.dimension.icon.height.md}",
            "type": "fontSizes"
          },
          "lg": {
            "value": "{component.slider.dimension.icon.height.lg}",
            "type": "fontSizes"
          }
        }
      },
      "config": {
        "track": {
          "height": {
            "value": "{app.dimension.control.height.lg} / 2",
            "type": "other",
            "description": "-0.5 for material design style (handle is offset)"
          },
          "step": {
            "value": "0.75rem",
            "type": "other"
          },
          "radius": {
            "default": {
              "value": "1.75rem",
              "type": "other",
              "description": "for 100% roundness make equal to height md"
            },
            "step": {
              "value": "0.25rem",
              "type": "other"
            }
          }
        },
        "handle": {
          "inset": {
            "value": "0.125rem",
            "type": "other"
          },
          "ratio": {
            "value": "1.25",
            "type": "other",
            "description": "ratio defines width → 1 = square / 1.5 = rectangle"
          },
          "pressed": {
            "value": "0.125",
            "type": "other"
          }
        }
      },
      "animate": {
        "handle": {
          "state": {
            "value": "0.5s ease-in-out",
            "type": "other"
          },
          "transform": {
            "value": "1s ease",
            "type": "other"
          }
        },
        "track": {
          "state": {
            "value": "0.5s ease",
            "type": "other"
          }
        }
      }
    }
  }
}