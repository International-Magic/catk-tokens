{
  "component": {
    "toggle": {
      "sizing": {
        "height": {
          "base": {
            "value": "16px",
            "type": "sizing"
          },
          "small": {
            "value": "{component.toggle.sizing.height.base} * 1.5",
            "type": "sizing"
          },
          "medium": {
            "value": "{component.toggle.sizing.height.base} * 1.75",
            "type": "sizing"
          },
          "large": {
            "value": "{component.toggle.sizing.height.base} * 2",
            "type": "sizing"
          }
        },
        "inset": {
          "default": {
            "value": "4px",
            "type": "sizing"
          }
        },
        "width": {
          "small": {
            "value": "{component.toggle.sizing.height.small} * 2 - {component.toggle.sizing.inset.default}",
            "type": "sizing"
          },
          "medium": {
            "value": "{component.toggle.sizing.height.medium} * 2 - {component.toggle.sizing.inset.default}",
            "type": "sizing"
          },
          "large": {
            "value": "{component.toggle.sizing.height.large} * 2 - {component.toggle.sizing.inset.default}",
            "type": "sizing"
          }
        }
      },
      "color": {
        "bg": {
          "active": {
            "type": "color",
            "value": "{theme.color.control.primary.bg.default}"
          },
          "inactive": {
            "type": "color",
            "value": "{theme.color.control.inactive.bg.default}"
          }
        },
        "fg": {
          "default": {
            "type": "color",
            "value": "{theme.color.control.primary.fg.on-primary}"
          }
        }
      },
      "box-shadow": {
        "default": {
          "value": "{theme.box-shadow.control.small}",
          "type": "boxShadow"
        }
      }
    },
    "toggle-nob": {
      "sizing": {
        "height": {
          "small": {
            "value": "{component.toggle.sizing.height.small} - {component.toggle.sizing.inset.default}",
            "type": "sizing"
          },
          "medium": {
            "value": "{component.toggle.sizing.height.medium} - {component.toggle.sizing.inset.default}",
            "type": "sizing"
          },
          "large": {
            "value": "{component.toggle.sizing.height.large} - {component.toggle.sizing.inset.default}",
            "type": "sizing"
          }
        }
      }
    }
  }
}