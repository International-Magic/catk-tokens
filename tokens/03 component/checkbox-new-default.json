{
  "component": {
    "checkbox": {
      "sizing": {
        "sm": {
          "value": "{component.checkbox.sizing.default} - {theme.config.scale.step.xs}",
          "type": "sizing"
        },
        "default": {
          "value": "{theme.sizing.control.xs} - {theme.config.scale.step.xs}",
          "type": "sizing"
        },
        "lg": {
          "value": "{component.checkbox.sizing.default} + {theme.config.scale.step.xs}",
          "type": "sizing"
        }
      },
      "border-width": {
        "default": {
          "value": "{theme.border-width.control.sm}",
          "type": "borderWidth"
        }
      },
      "border-radius": {
        "sm": {
          "value": "{component.checkbox.border-radius.default} - {theme.config.scale.step.2xs}",
          "type": "borderRadius"
        },
        "default": {
          "value": "{theme.border-radius.control.xs} - {theme.config.scale.step.3xs}",
          "type": "borderRadius"
        },
        "lg": {
          "value": "{component.checkbox.border-radius.default} + {theme.config.scale.step.2xs}",
          "type": "borderRadius"
        }
      },
      "typography": {
        "symbol": {
          "sm": {
            "value": "{theme.typography.control.strong.symbol.xs}",
