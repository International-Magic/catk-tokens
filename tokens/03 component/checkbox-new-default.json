              }
            },
            "active": {
              "border": {
                "value": "{theme.color.control.focus.border.active}",
                "type": "color"
              }
            }
          },
          "unchecked": {
            "default": {
              "border": {
                "value": "{theme.color.control.inactive.fg.muted.default}",
                "type": "color"
              }
            },
            "fill": {
