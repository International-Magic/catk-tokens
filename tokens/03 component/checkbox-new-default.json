          "default": {
            "value": "2px",
            "type": "spacing"
          }
        }
      },
      "symbol": {
        "sm": {
          "value": "{component.checkbox.symbol.default} - {theme.config.scale.step.xs}",
          "type": "sizing"
        },
        "default": {
          "value": "{theme.sizing.control.2xs}",
          "type": "sizing"
        },
        "lg": {
          "value": "{component.checkbox.symbol.default} + {theme.config.scale.step.xs}",
          "type": "sizing"
        }
      },
